{
  "cSpell.words": [
    "alllayers",
    "arcgiscache",
    "dask",
    "EXTENTSFGDB",
    "gethostbyname",
    "globbing",
    "iglob",
    "NAIP",
    "propy",
    "pygsheets",
    "pytz",
    "sendgrid",
    "vtpk"
  ],
  "[python]": {
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit",
      "source.organizeImports": "explicit"
    },
    "editor.defaultFormatter": "ms-python.black-formatter",
  },
  "python.formatting.provider": "none",
}
